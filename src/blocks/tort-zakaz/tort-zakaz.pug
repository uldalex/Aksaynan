                            li.product-card__ves-item 2.5кг
                            li.product-card__ves-item.some-ves свой вес
                            input(type="text" value="" class="some-ves-input")
                            img.close(src="img/close.svg")
                            input(type="hidden" value="" id="product-card-ves1")
                        .product-card__kol
                          ul.product-card__kol-list
                            li.product-card__kol-item
                              .quantity
                                .quantity-plus +
                                .quantity-val
                                  input(type="number" value="1" step="1" min="1" max="")
