//- Все примеси в этом файле должны начинаться c имени блока (tort-zakaz)

mixin tort-zakaz(text, mods)

  //- Принимает:
  //-   text    {string} - текст
  //-   mods    {string} - список модификаторов
  //- Вызов:
        +tort-zakaz('Текст', 'some-mod')

  -
    // список модификаторов
    var allMods = '';
    if(typeof(mods) !== 'undefined' && mods) {
      var modsList = mods.split(',');
      for (var i = 0; i < modsList.length; i++) {
        allMods = allMods + ' tort-zakaz--' + modsList[i].trim();
      }
    }

  .tort-zakaz(class=allMods)&attributes(attributes)
    img.parallax.parallax7(src="img/parallax7.png") 
    .tort-zakaz__inner
      h3 Торты на заказ
      .d-xs-none.tort-zakaz__filter-toggler
        img(src="img/filter.svg" width="40px")
      ul.tort-zakaz__sort
        li.tort-zakaz-cat.alt-btn.active(data-cat=".cat1") Праздничные
        li.tort-zakaz-cat.alt-btn(data-cat=".cat2") Свадебные
        li.tort-zakaz-cat.alt-btn(data-cat=".cat3") Детские
        li.tort-zakaz-cat.alt-btn(data-cat=".cat4") На день рождения
        li.tort-zakaz-cat.alt-btn(data-cat=".cat5") Корпоративные
        li.tort-zakaz-cat.alt-btn(data-cat=".cat6") Эксклюзивные
      .tort-zakaz__filter
        .product-card.cat.cat1.cat5.active
